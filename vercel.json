{
    "version": 2,
    "framework": "nuxtjs",
    "buildCommand": "NODE_OPTIONS='--experimental-json-modules' pnpm build",
    "installCommand": "pnpm install",
    "outputDirectory": ".output",
    "builds": [
        {
            "src": ".output/server/index.mjs",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    ".output/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/_build/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/.output/public/_build/$1"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/.output/public/assets/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/.output/server/index.mjs"
        }
    ]
}