{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "pnpm build",
                "outputDirectory": ".output/public"
            }
        }
    ],
    "routes": [
        {
            "src": "/_build/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/_build/$1"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/_server/index.mjs"
        }
    ],
    "regions": [
        "iad1"
    ]
}