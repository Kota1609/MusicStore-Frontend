{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "NODE_OPTIONS='--experimental-json-modules' pnpm build",
                "outputDirectory": ".output/public",
                "installCommand": "pnpm install"
            }
        },
        {
            "src": ".output/server/index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/_build/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/_build/$1"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/.output/server/index.mjs"
        }
    ]
}